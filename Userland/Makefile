include Makefile.inc

SAMPLE_DATA=0001-sampleDataModule.bin

all: sampleCodeModule sampleDataModule

sampleCodeModule:
	cd SampleCodeModule; make

sampleDataModule:
	printf "E522E522F522H522H522F522E522C522A522A522C522E522E542C511C544E522E522F522H522H522F522E522C522A522A522C522E522C542A511A544C522C522E522A522C522E511F511E522A522C522E511F511E522C522A522C522H422E561E512F522H522H522F522E522C522A522A522C522E522C542A511A544" >> $(SAMPLE_DATA) && dd if=/dev/zero bs=1 count=1 >> $(SAMPLE_DATA)

clean:
	cd SampleCodeModule; make clean
	rm -rf *.bin


.PHONY: sampleCodeModule all clean
