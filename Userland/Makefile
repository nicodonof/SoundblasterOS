include Makefile.inc

SAMPLE_DATA=0001-sampleDataModule.bin

all: sampleCodeModule sampleDataModule

sampleCodeModule:
	cd SampleCodeModule; make

sampleDataModule:
	printf "D522D522F522H522H522F522D522C522A522A522C522D522D542C511C544D522D522F522H522H522F522D522C522A522A522C522D522C542A511A544C522C522D522A522C522D511F511D522A522C522D511F511D522C522A522C522H422D561D512F522H522H522F522D522C522A522A522C522D522C542A511A544" >> $(SAMPLE_DATA) && dd if=/dev/zero bs=1 count=1 >> $(SAMPLE_DATA)

clean:
	cd SampleCodeModule; make clean
	rm -rf *.bin


.PHONY: sampleCodeModule all clean
